<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- 前台 -->
	<bean id="index" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true" />
		<property name="url" value="/index.jsp" />
	</bean>

	<bean id="member.sign-in" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true" />
		<property name="url" value="/member/sign-in.jsp" />
	</bean>
	
	<bean id="member.sign-in-fail" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true" />
		<property name="url" value="/member/sign-in-fail.jsp" />
	</bean>

	<bean id="member.sign-up" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true" />
		<property name="url" value="/member/sign-up.jsp" />
	</bean>

	<bean id="member.update" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true" />
		<property name="url" value="/member/update.jsp" />
	</bean>

	<bean id="member.update-password" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true" />
		<property name="url" value="/member/update-password.jsp" />
	</bean>

	<!-- 後台 -->
	<bean id="member.search" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/member/search.jsp" />
	</bean>

	<bean id="country.insert" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true" />
		<property name="url" value="/country/insert.jsp" />
	</bean>

	<bean id="city.insert" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true" />
		<property name="url" value="/city/insert.jsp" />
	</bean>

	<bean id="clinic.insert" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true" />
		<property name="url" value="/clinic/insert.jsp" />
	</bean>

	<bean id="clinic.search" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/clinic/search.jsp" />
	</bean>

</beans>